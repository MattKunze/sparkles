{
  "name": "sparkles",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:app dev:wss",
    "dev:app": "next dev",
    "dev:wss": "tsx --watch src/server/wsServer.ts",
    "docker-compose:up": "docker-compose --env-file .env.local up -d",
    "docker-compose:down": "docker-compose down",
    "build": "next build",
    "build:kernel-nodejs": "docker build kernels/nodejs/ -t sparkles:kernel-nodejs",
    "prebuild:kernel-nodejs": "docker ps -a -q --filter \"label=sparkles.owner\" | xargs docker rm -f",
    "start": "run-p start:*",
    "start:app": "next start",
    "start:wss": "tsx src/server/wsServer.ts",
    "lint": "next lint",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@dnd-kit/core": "6.1.0",
    "@monaco-editor/react": "^4.5.2",
    "@nivo/bar": "^0.84.0",
    "@nivo/calendar": "^0.84.0",
    "@nivo/core": "^0.84.0",
    "@nivo/line": "^0.84.0",
    "@tailwindcss/typography": "0.5.10",
    "@tanstack/react-query": "^4.35.3",
    "@tanstack/react-query-devtools": "^4.35.3",
    "@trpc/client": "^10.38.3",
    "@trpc/next": "^10.38.3",
    "@trpc/react-query": "^10.38.3",
    "@trpc/server": "^10.38.3",
    "@uidotdev/usehooks": "^2.3.1",
    "bufferutil": "^4.0.7",
    "chance": "1.1.11",
    "chokidar": "^3.5.3",
    "cls-hooked": "^4.2.2",
    "clsx": "^2.0.0",
    "daisyui": "^4.4.20",
    "dockerode": "4.0.0",
    "dotenv": "^16.3.1",
    "immer": "^10.0.2",
    "lodash": "^4.17.21",
    "next": "^13.4.19",
    "next-auth": "^4.24.5",
    "postcss": "^8.4.29",
    "prettier": "^3.1.0",
    "queue": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "surrealdb.js": "^0.11.0",
    "tailwindcss": "^3.3.3",
    "ulid": "^2.3.0",
    "utf-8-validate": "^6.0.3",
    "yargs": "^17.7.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/chance": "1.1.6",
    "@types/cls-hooked": "4.3.8",
    "@types/dockerode": "3.3.23",
    "@types/lodash": "4.14.202",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/yargs": "17.0.32",
    "autoprefixer": "10.4.15",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "node-loader": "2.0.0",
    "npm-run-all": "4.1.5",
    "ts-essentials": "9.4.0",
    "tsx": "4.7.0",
    "typescript": "5.3.3"
  }
}
